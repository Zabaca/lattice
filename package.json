{
  "name": "@zabaca/lattice",
  "version": "0.1.0",
  "description": "Human-initiated, AI-powered knowledge graph for markdown documentation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "lattice": "dist/cli.js"
  },
  "scripts": {
    "build": "bun build src/cli.ts --outdir dist --target node --splitting --format cjs",
    "dev": "bun run src/main.ts",
    "test": "bun test",
    "lattice": "bun run src/main.ts",
    "release:dry": "npm publish --dry-run",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "knowledge-graph",
    "falkordb",
    "documentation",
    "markdown",
    "ai",
    "embeddings",
    "semantic-search"
  ],
  "author": "Zabaca",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zabaca/lattice.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/websockets": "^11.1.9",
    "chalk": "^5.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "commander": "^12.0.0",
    "glob": "^10.3.0",
    "gray-matter": "^4.0.3",
    "ioredis": "^5.3.0",
    "nest-commander": "^3.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "voyageai": "^0.0.3-alpha",
    "yaml": "^2.3.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "bun-types": "^1.0.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "optionalDependencies": {
    "@grpc/grpc-js": "^1.14.1",
    "@grpc/proto-loader": "^0.8.0",
    "@nestjs/platform-socket.io": "^11.1.9",
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9",
    "kafkajs": "^2.2.4",
    "mqtt": "^5.14.1",
    "nats": "^2.29.3"
  }
}
